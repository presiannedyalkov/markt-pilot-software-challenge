    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode production",
    "build:mock": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build --mode mock",
    "test:unit": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development vue-cli-service test:unit",
    "lint": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development vue-cli-service lint",
    "inspect": "vue-cli-service inspect > current-webpack-settings.js"
  },
  "dependencies": {
    "axios": "^0.25.0",
